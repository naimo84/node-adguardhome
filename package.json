{
  "name": "adguardhome",
  "version": "0.0.2",
  "description": "OpenAPI client for adguardhome",
  "author": "naimo84",
  "repository": {
    "type": "git",
    "url": "https://github.com/naimo84/adguardhome.git"
  },
  "keywords": [
    "fetch",
    "typescript",
    "adguardhome",
    "adguard",
    "openapi-client",
    "openapi-generator"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "type": "module",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "release": "release-it",
    "format": "npx @biomejs/biome format --write *.ts",
    "lint": "npx @biomejs/biome lint --apply *.ts"
  },
  "dependencies": {
    "node-fetch": "^2.6.0",
    "@types/node-fetch": "^2.5.7",
    "@types/node": "*",
    "form-data": "^2.5.0",
    "es6-promise": "^4.2.4",
    "url-parse": "^1.4.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.3",
    "typescript": "^4.0",
    "@types/url-parse": "1.4.4",
    "release-it": "^14.11.8"
  },
  "funding": {
    "type": "paypal",
    "url": "https://paypal.me/NeumannBenjamin"
  }
}
